[32m+[m[32m                textInputComponent.setLayoutParams(layoutParams);[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            lastView = textInputComponent;[m
[32m+[m[32m            activeDataComponents.add(CustomComponents.TEXT_INPUT);[m
