[32m+[m[32m    private void Send()[m
[32m+[m[32m    {[m
[32m+[m[32m        for (int i = 0; i < activeViews.size(); i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            if (activeViews.get(i) != null)[m
[32m+[m[32m            {[m
[32m+[m[32m                CustomComponents currentComponent = activeDataComponents.get(i);[m
[32m+[m[32m                AddForComponent(activeViews.get(i), currentComponent);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        dataPacket.Send(getContext());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private boolean ComponentExists(CustomComponents customComponent)[m
[32m+[m[32m    {[m
[32m+[m[32m        return activeDataComponents.contains(customComponent);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void AddForComponent(View activeView, CustomComponents customComponent)[m
[32m+[m[32m    {[m
[32m+[m[32m        switch (customComponent)[m
[32m+[m[32m        {[m
[32m+[m[32m            case TEXT_INPUT:[m
[32m+[m[32m                TextData textData = ((TextInputComponent)activeView).getTextData();[m
[32m+[m[32m                dataPacket.addTextData(textData);[m
[32m+[m[32m                break;[m
[32m+[m[32m            default: ShowAlertDialog(customComponent.name() + " is not a valid component."); break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void HandleItemClick(int dataId)[m
[32m+[m[32m    {[m
[32m+[m[32m        txtAddDataHint.setVisibility(View.INVISIBLE);[m
[32m+[m
[32m+[m[32m        switch (dataId)[m
[32m+[m[32m        {[m
[32m+[m[32m            case 0: Send(); break;[m
[32m+[m[32m            case 1: AddTextInput(); break;[m
[32m+[m[32m            default: break;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
